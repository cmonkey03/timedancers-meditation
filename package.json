{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "timedancers-meditation",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "vitest --run",
    "detox:build": "detox build -c ios.debug",
    "detox:test": "detox test -c ios.debug",
    "detox:test:meditation": "detox test -c ios.debug e2e/meditation.e2e.ts",
    "detox:test:settings": "detox test -c ios.debug e2e/settings.e2e.ts",
    "detox:test:onboarding": "detox test -c ios.debug e2e/onboarding.e2e.ts",
    "e2e": "./scripts/run-e2e-tests.sh",
    "e2e:verbose": "./scripts/run-e2e-tests.sh --verbose",
    "e2e:skip-build": "./scripts/run-e2e-tests.sh --skip-build",
    "e2e:skip-unit": "./scripts/run-e2e-tests.sh --skip-unit",
    "e2e:meditation": "./scripts/run-e2e-tests.sh --suite meditation",
    "e2e:settings": "./scripts/run-e2e-tests.sh --suite settings",
    "e2e:onboarding": "./scripts/run-e2e-tests.sh --suite onboarding",
    "test:all": "./scripts/run-e2e-tests.sh",
    "test:unit-only": "npm test",
    "test:e2e-only": "./scripts/run-e2e-tests.sh --skip-unit"
  },
  "dependencies": {
    "@expo-google-fonts/cinzel": "^0.4.2",
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/material-top-tabs": "^7.3.7",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.8",
    "expo-audio": "~1.0.13",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-keep-awake": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.11",
    "expo-router": "~6.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-onboarding-swiper": "^1.3.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-tab-view": "^4.1.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.0",
    "@types/react-native-onboarding-swiper": "^1.1.9",
    "@vitest/ui": "^3.2.4",
    "detox": "^20.14.9",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  },
  "private": true
}
